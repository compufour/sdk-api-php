{
    "name": "compufacil/api-sdk",
    "description": "Compufácil SDK for PHP",
    "keywords": ["compufacil", "sdk"],
    "type": "library",
    "homepage": "https://github.com/compufour/php-api-sdk",
    "license": "Compufácil Platform",
    "authors": [
        {
            "name": "Compufácil",
            "homepage": "https://github.com/compufour/php-api-sdk/contributors"
        }
    ],
    "require": {
        "php": "^7.0"
    },
    "require-dev": {
        "phpunit/phpunit": "~6.0"
    },
    "autoload": {
        "psr-4": {
            "Compufacil\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "CompufacilTest\\": "tests/"
        }
    },
    "scripts": {
        "tests": "./vendor/bin/phpunit tests --colors=always"
    }
}
